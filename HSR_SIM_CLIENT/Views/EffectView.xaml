<UserControl x:Class="HSR_SIM_CLIENT.Views.EffectView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:HSR_SIM_CLIENT.Views"
             mc:Ignorable="d"
             Name="Parent"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid DataContext="{Binding ElementName=Parent}">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel  Grid.Row="0" Orientation="Horizontal">
          
            <Label Content="{Binding  ViewModel.Name}" />
            <Label Content="Value=" />
            <Label Content="{Binding  ViewModel.Value}" />
            <Label Content="DynamicValue=" />
            <Label Content="{Binding  ViewModel.DynamicValue}" />
        </StackPanel>
    </Grid>
</UserControl>
